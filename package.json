    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.5.1",
    "jsdom": "^19.0.0",
    "jsdom-global": "^3.0.2",
    "prettier": "^2.5.1",
    "raw-loader": "^4.0.2",
